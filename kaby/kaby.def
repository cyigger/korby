; Definition file for player
; Contains all the filenames needed for the character

; Player information
[Info]
name = "kaby"        ;Name of character
displayname = "Korby" ;Name of character to display
versiondate = 3/12/2022   ;Version date of character
mugenversion = 1.1  ;Version of M.U.G.E.N character works on
ikemenVersion = 0.98
author = "KBB Dev Team"      ;Character author name
pal.defaults = 1,2,3,4,5,6,7,8   ;Default palettes in order of preference

; Files for the player
[Files]
cmd     = kaby.cmd   ;Command set
cns     = kaby.cns   ;Constants
st      = kaby.cns   ;States
st2     = Specials.cns
;st3     = StarBurst.cns
;st4   = Power Delay.cns
;st5     = StarStrike.cns
;st6     = HurtStates.cns
st7     = seethe.cns
stcommon = common1.cns ;Common states
sprite  = kaby.sff   ;Sprite
anim    = kaby.air   ;Animation
sound  = kaby.snd   ;Sound
movelist = movelist.dat

[Palette Keymap]
a = 1
b = 1
c = 1
x = 1
y = 1
z = 1